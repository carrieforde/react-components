{
  "name": "npm-react-pkg-template",
  "version": "1.0.0",
  "description": "A starter project for creating a React component package",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:storybook": "build-storybook",
    "dev": "start-storybook -p 6006",
    "eslint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "eslint:fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "prettier": "prettier './src/**/*.{js,jsx,ts,tsx}'",
    "stylelint": " stylelint './src/**/*.{css,scss}'",
    "stylelint:fix": " stylelint --fix './src/**/*.{css,scss}'",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carrieforde/npm-react-pkg-template.git"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "carrieforde",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carrieforde/npm-react-pkg-template/issues"
  },
  "homepage": "https://github.com/carrieforde/npm-react-pkg-template#readme",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@storybook/addon-a11y": "^6.1.2",
    "@storybook/addon-actions": "^6.1.2",
    "@storybook/addon-essentials": "^6.1.2",
    "@storybook/addon-links": "^6.1.2",
    "@storybook/react": "^6.1.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-loader": "^8.2.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.33.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.29.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
